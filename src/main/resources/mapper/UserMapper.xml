<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shoulder.mapper.UserMapper">
    <select id="findEntityById" parameterType="Integer" resultType="com.shoulder.model.User">
        select id, username, email, telephone from user where id=#{id}
    </select>
</mapper>